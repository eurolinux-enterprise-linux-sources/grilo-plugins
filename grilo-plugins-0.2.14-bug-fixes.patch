       /* Fetch web API in order to update current cache */
       thetvdb_execute_resolve_web (os);
       return;
@@ -1444,7 +1445,12 @@ cache_find_serie_done (GObject *object,
   if (resource == NULL) {
     GRL_DEBUG ("[Series] Cache miss with '%s' due '%s'", show, err->message);
     g_error_free (err);
